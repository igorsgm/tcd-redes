<?xml version="1.0" encoding="utf-8"?>
<form>
  <fieldset>
    <field name="id" type="text" readonly="true" class="readonly" default="0" description="JGLOBAL_FIELD_ID_DESC"/>
    <field name="ordering"/>
    <field name="state" default="1" type="list" label="JSTATUS" description="JFIELD_PUBLISHED_DESC" class="inputbox" size="1">
      <option value="1">JPUBLISHED</option>
      <option value="0">JUNPUBLISHED</option>
      <option value="2">JARCHIVED</option>
      <option value="-2">JTRASHED</option>
    </field>
    <field name="checked_out" type="hidden" filter="unset"/>
    <field name="checked_out_time" default="0000-00-00 00:00:00" type="hidden" filter="unset"/>
    <field name="identificador_aparelho" filter="safehtml" type="text" label="COM_DISPOSITIVOS_FORM_LBL_DISPOSITIVO_IDENTIFICADOR_APARELHO" description="COM_DISPOSITIVOS_FORM_DESC_DISPOSITIVO_IDENTIFICADOR_APARELHO" hint="COM_DISPOSITIVOS_FORM_LBL_DISPOSITIVO_IDENTIFICADOR_APARELHO"/>
    <field name="tipo" extension="com_dispositivos" type="category" label="COM_DISPOSITIVOS_FORM_LBL_DISPOSITIVO_TIPO" description="COM_DISPOSITIVOS_FORM_DESC_DISPOSITIVO_TIPO" hint="COM_DISPOSITIVOS_FORM_LBL_DISPOSITIVO_TIPO"/>
    <field name="modelo" type="text" required="true" label="COM_DISPOSITIVOS_FORM_LBL_DISPOSITIVO_MODELO" description="COM_DISPOSITIVOS_FORM_DESC_DISPOSITIVO_MODELO" hint="COM_DISPOSITIVOS_FORM_LBL_DISPOSITIVO_MODELO"/>
    <field name="sistema_operacional" filter="safehtml" type="text" label="COM_DISPOSITIVOS_FORM_LBL_DISPOSITIVO_SISTEMA_OPERACIONAL" description="COM_DISPOSITIVOS_FORM_DESC_DISPOSITIVO_SISTEMA_OPERACIONAL" hint="COM_DISPOSITIVOS_FORM_LBL_DISPOSITIVO_SISTEMA_OPERACIONAL"/>
    <field name="nome_propiertario" filter="safehtml" type="text" label="COM_DISPOSITIVOS_FORM_LBL_DISPOSITIVO_NOME_PROPIERTARIO" description="COM_DISPOSITIVOS_FORM_DESC_DISPOSITIVO_NOME_PROPIERTARIO" hint="COM_DISPOSITIVOS_FORM_LBL_DISPOSITIVO_NOME_PROPIERTARIO"/>
    <field name="version_note" type="text" label="JGLOBAL_FIELD_VERSION_NOTE_LABEL" description="JGLOBAL_FIELD_VERSION_NOTE_DESC" class="inputbox" size="45" labelclass="control-label"/>
  </fieldset>
</form>
